{
	"servers": {
		"io.github.ChromeDevTools/chrome-devtools-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"chrome-devtools-mcp@0.10.2"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "0.10.2"
		},
		"io.github.upstash/context7": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@upstash/context7-mcp@1.0.31"
			],
			"env": {
				"CONTEXT7_API_KEY": "${input:CONTEXT7_API_KEY}"
			},
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.31"
		},
		"firecrawl/firecrawl-mcp-server": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"firecrawl-mcp@latest"
			],
			"env": {
				"FIRECRAWL_API_KEY": "${input:api_key}"
			},
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.0"
		},
		"com.postman/postman-mcp-server": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@postman/postman-mcp-server@2.5.1"
			],
			"env": {
				"POSTMAN_API_KEY": "${input:POSTMAN_API_KEY}"
			},
			"gallery": "https://api.mcp.github.com",
			"version": "2.5.1"
		},
		"cognitionai/deepwiki": {
			"type": "http",
			"url": "https://mcp.deepwiki.com/sse",
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.0"
		}
	},
	"inputs": [
		{
			"id": "CONTEXT7_API_KEY",
			"type": "promptString",
			"description": "API key for authentication",
			"password": true
		},
		{
			"id": "api_key",
			"type": "promptString",
			"description": "your API key",
			"password": true
		},
		{
			"id": "POSTMAN_API_KEY",
			"type": "promptString",
			"description": "A valid Postman API key.",
			"password": true
		}
	]
}